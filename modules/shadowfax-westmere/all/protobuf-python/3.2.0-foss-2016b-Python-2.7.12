#%Module
proc ModulesHelp { } {
    puts stderr { Python Protocol Buffers runtime library. - Homepage: https://github.com/google/protobuf/
    }
}

module-whatis {Description: Python Protocol Buffers runtime library. - Homepage: https://github.com/google/protobuf/}

set root /apps/easybuild/software/shadowfax-westmere/protobuf-python/3.2.0-foss-2016b-Python-2.7.12

conflict protobuf-python

if { ![ is-loaded foss/2016b ] } {
    module load foss/2016b
}

if { ![ is-loaded Python/2.7.12-foss-2016b ] } {
    module load Python/2.7.12-foss-2016b
}

if { ![ is-loaded protobuf/3.2.0-foss-2016b ] } {
    module load protobuf/3.2.0-foss-2016b
}

prepend-path	PATH		$root/bin
setenv	EBROOTPROTOBUFMINPYTHON		"$root"
setenv	EBVERSIONPROTOBUFMINPYTHON		"3.2.0"
setenv	EBDEVELPROTOBUFMINPYTHON		"$root/easybuild/protobuf-python-3.2.0-foss-2016b-Python-2.7.12-easybuild-devel"

prepend-path	PYTHONPATH		$root/lib/python2.7/site-packages
# Built with EasyBuild version 3.1.2
