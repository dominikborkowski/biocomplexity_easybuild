#%Module
proc ModulesHelp { } {
    puts stderr {

Description
===========
pkgconfig is a Python module to interface with the pkg-config command line tool


More information
================
 - Homepage: http://github.com/matze/pkgconfig
    }
}

module-whatis {Description: pkgconfig is a Python module to interface with the pkg-config command line tool}
module-whatis {Homepage: http://github.com/matze/pkgconfig}

set root /apps/easybuild/software/orion-broadwell/pkgconfig/1.3.1-foss-2018b-Python-3.6.6

conflict pkgconfig

if { ![ is-loaded foss/2018b ] } {
    module load foss/2018b
}

if { ![ is-loaded Python/3.6.6-foss-2018b ] } {
    module load Python/3.6.6-foss-2018b
}

if { ![ is-loaded pkg-config/0.29.2-GCCcore-7.3.0 ] } {
    module load pkg-config/0.29.2-GCCcore-7.3.0
}

setenv	EBROOTPKGCONFIG		"$root"
setenv	EBVERSIONPKGCONFIG		"1.3.1"
setenv	EBDEVELPKGCONFIG		"$root/easybuild/pkgconfig-1.3.1-foss-2018b-Python-3.6.6-easybuild-devel"

prepend-path	PYTHONPATH		$root/lib/python3.6/site-packages
