    module load Python/3.6.4-foss-2018a
}

setenv	EBROOTSPICEYPY		"$root"
setenv	EBVERSIONSPICEYPY		"2.1.0"
setenv	EBDEVELSPICEYPY		"$root/easybuild/SpiceyPy-2.1.0-foss-2018a-Python-3.6.4-easybuild-devel"

