prepend-path	PATH		$root/bin
setenv	EBROOTJAVA		"$root"
setenv	EBVERSIONJAVA		"11.0.2"
setenv	EBDEVELJAVA		"$root/easybuild/Java-11.0.2-easybuild-devel"

prepend-path	PATH		$root
setenv	JAVA_HOME		"/apps/easybuild/software/pegasus-sandy_bridge/Java/11.0.2"
