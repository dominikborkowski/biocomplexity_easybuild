set root /apps/easybuild/software/pegasus-sandy_bridge/scikit-image/0.13.1-foss-2018a-Python-3.6.4

conflict scikit-image

if { ![ is-loaded foss/2018a ] } {
    module load foss/2018a
}

if { ![ is-loaded Python/3.6.4-foss-2018a ] } {
    module load Python/3.6.4-foss-2018a
}

if { ![ is-loaded Qhull/2015.2-GCCcore-6.4.0 ] } {
    module load Qhull/2015.2-GCCcore-6.4.0
}

if { ![ is-loaded matplotlib/2.1.2-foss-2018a-Python-3.6.4 ] } {
    module load matplotlib/2.1.2-foss-2018a-Python-3.6.4
}

if { ![ is-loaded Pillow/5.0.0-foss-2018a-Python-3.6.4 ] } {
    module load Pillow/5.0.0-foss-2018a-Python-3.6.4
}

prepend-path	LD_LIBRARY_PATH		$root/lib
prepend-path	LIBRARY_PATH		$root/lib
prepend-path	PATH		$root/bin
setenv	EBROOTSCIKITMINIMAGE		"$root"
setenv	EBVERSIONSCIKITMINIMAGE		"0.13.1"
