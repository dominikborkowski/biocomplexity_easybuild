#%Module
proc ModulesHelp { } {
    puts stderr {

Description
===========
Python Protocol Buffers runtime library.


More information
================
 - Homepage: https://github.com/google/protobuf/
    }
}

module-whatis {Description: Python Protocol Buffers runtime library.}
module-whatis {Homepage: https://github.com/google/protobuf/}

set root /apps/easybuild/software/pegasus-sandy_bridge/protobuf-python/3.3.0-foss-2017a-Python-3.6.1

conflict protobuf-python

if { ![ is-loaded foss/2017a ] } {
    module load foss/2017a
}

if { ![ is-loaded Python/3.6.1-foss-2017a ] } {
    module load Python/3.6.1-foss-2017a
}

if { ![ is-loaded protobuf/3.3.0-foss-2017a ] } {
    module load protobuf/3.3.0-foss-2017a
}

prepend-path	PATH		$root/bin
setenv	EBROOTPROTOBUFMINPYTHON		"$root"
setenv	EBVERSIONPROTOBUFMINPYTHON		"3.3.0"
setenv	EBDEVELPROTOBUFMINPYTHON		"$root/easybuild/protobuf-python-3.3.0-foss-2017a-Python-3.6.1-easybuild-devel"

prepend-path	PYTHONPATH		$root/lib/python3.6/site-packages
