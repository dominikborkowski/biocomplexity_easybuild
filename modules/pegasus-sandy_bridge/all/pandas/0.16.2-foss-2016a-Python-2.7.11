set root /apps/easybuild/software/pegasus-sandy_bridge/pandas/0.16.2-foss-2016a-Python-2.7.11

conflict pandas

if { ![ is-loaded foss/2016a ] } {
    module load foss/2016a
}

if { ![ is-loaded Python/2.7.11-foss-2016a ] } {
    module load Python/2.7.11-foss-2016a
}

setenv	EBROOTPANDAS		"$root"
setenv	EBVERSIONPANDAS		"0.16.2"
setenv	EBDEVELPANDAS		"$root/easybuild/pandas-0.16.2-foss-2016a-Python-2.7.11-easybuild-devel"

prepend-path	PYTHONPATH		$root/lib/python2.7/site-packages
