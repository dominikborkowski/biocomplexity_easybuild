#%Module
proc ModulesHelp { } {
    puts stderr {

Description
===========
binutils: GNU binary utilities


More information
================
 - Homepage: http://directory.fsf.org/project/binutils/
    }
}

module-whatis {Description: binutils: GNU binary utilities}
module-whatis {Homepage: http://directory.fsf.org/project/binutils/}

set root /apps/easybuild/software/pegasus-sandy_bridge/binutils/2.30-GCCcore-7.3.0

conflict binutils

if { ![ is-loaded GCCcore/7.3.0 ] } {
    module load GCCcore/7.3.0
}

prepend-path	CPATH		$root/include
prepend-path	LD_LIBRARY_PATH		$root/lib
prepend-path	LIBRARY_PATH		$root/lib
prepend-path	MANPATH		$root/share/man
prepend-path	PATH		$root/bin
setenv	EBROOTBINUTILS		"$root"
setenv	EBVERSIONBINUTILS		"2.30"
setenv	EBDEVELBINUTILS		"$root/easybuild/binutils-2.30-GCCcore-7.3.0-easybuild-devel"

# Built with EasyBuild version 3.6.2
