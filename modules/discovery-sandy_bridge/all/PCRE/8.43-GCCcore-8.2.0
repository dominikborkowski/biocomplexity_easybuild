================
 - Homepage: http://www.pcre.org/
    }
}

module-whatis {Description: 
 The PCRE library is a set of functions that implement regular expression
 pattern matching using the same syntax and semantics as Perl 5.
}
module-whatis {Homepage: http://www.pcre.org/}

set root /apps/easybuild/software/discovery-sandy_bridge/PCRE/8.43-GCCcore-8.2.0

conflict PCRE

if { ![ is-loaded GCCcore/8.2.0 ] } {
    module load GCCcore/8.2.0
}

if { ![ is-loaded bzip2/1.0.6-GCCcore-8.2.0 ] } {
    module load bzip2/1.0.6-GCCcore-8.2.0
}

if { ![ is-loaded zlib/1.2.11-GCCcore-8.2.0 ] } {
    module load zlib/1.2.11-GCCcore-8.2.0
}

prepend-path	CPATH		$root/include
prepend-path	LD_LIBRARY_PATH		$root/lib
prepend-path	LIBRARY_PATH		$root/lib
prepend-path	MANPATH		$root/share/man
prepend-path	PATH		$root/bin
prepend-path	PKG_CONFIG_PATH		$root/lib/pkgconfig
setenv	EBROOTPCRE		"$root"
setenv	EBVERSIONPCRE		"8.43"
setenv	EBDEVELPCRE		"$root/easybuild/PCRE-8.43-GCCcore-8.2.0-easybuild-devel"

