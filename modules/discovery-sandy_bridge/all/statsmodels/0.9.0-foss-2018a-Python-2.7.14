#%Module
proc ModulesHelp { } {
    puts stderr {

Description
===========
Statsmodels is a Python module that allows users to explore data, estimate statistical models,
and perform statistical tests.


More information
================
 - Homepage: http://statsmodels.sourceforge.net/


Included extensions
===================
patsy-0.4.1, statsmodels-0.9.0
    }
}

module-whatis {Description: Statsmodels is a Python module that allows users to explore data, estimate statistical models,
and perform statistical tests.}
module-whatis {Homepage: http://statsmodels.sourceforge.net/}
module-whatis {Extensions: patsy-0.4.1, statsmodels-0.9.0}

set root /apps/easybuild/software/discovery-sandy_bridge/statsmodels/0.9.0-foss-2018a-Python-2.7.14

conflict statsmodels

if { ![ is-loaded foss/2018a ] } {
    module load foss/2018a
}

if { ![ is-loaded Python/2.7.14-foss-2018a ] } {
    module load Python/2.7.14-foss-2018a
}

prepend-path	LD_LIBRARY_PATH		$root/lib
prepend-path	LIBRARY_PATH		$root/lib
prepend-path	PATH		$root/bin
setenv	EBROOTSTATSMODELS		"$root"
setenv	EBVERSIONSTATSMODELS		"0.9.0"
setenv	EBDEVELSTATSMODELS		"$root/easybuild/statsmodels-0.9.0-foss-2018a-Python-2.7.14-easybuild-devel"

prepend-path	PYTHONPATH		$root/lib/python2.7/site-packages
# Built with EasyBuild version 3.6.2
setenv	EBEXTSLISTSTATSMODELS		"patsy-0.4.1,statsmodels-0.9.0"
