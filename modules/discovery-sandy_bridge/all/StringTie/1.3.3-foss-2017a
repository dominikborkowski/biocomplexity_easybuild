#%Module
proc ModulesHelp { } {
    puts stderr {

Description
===========
StringTie is a fast and highly efficient assembler of RNA-Seq alignments into potential transcripts.


More information
================
 - Homepage: http://ccb.jhu.edu/software/stringtie/
    }
}

module-whatis {Description: StringTie is a fast and highly efficient assembler of RNA-Seq alignments into potential transcripts.}
module-whatis {Homepage: http://ccb.jhu.edu/software/stringtie/}

set root /apps/easybuild/software/discovery-sandy_bridge/StringTie/1.3.3-foss-2017a

conflict StringTie

if { ![ is-loaded foss/2017a ] } {
    module load foss/2017a
}

prepend-path	PATH		$root/bin
setenv	EBROOTSTRINGTIE		"$root"
setenv	EBVERSIONSTRINGTIE		"1.3.3"
setenv	EBDEVELSTRINGTIE		"$root/easybuild/StringTie-1.3.3-foss-2017a-easybuild-devel"

