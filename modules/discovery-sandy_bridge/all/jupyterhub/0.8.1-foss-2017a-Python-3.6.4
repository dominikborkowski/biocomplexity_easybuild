set root /apps/easybuild/software/discovery-sandy_bridge/jupyterhub/0.8.1-foss-2017a-Python-3.6.4

conflict jupyterhub

if { ![ is-loaded foss/2017a ] } {
    module load foss/2017a
}

if { ![ is-loaded Python/3.6.4-foss-2017a ] } {
    module load Python/3.6.4-foss-2017a
}

