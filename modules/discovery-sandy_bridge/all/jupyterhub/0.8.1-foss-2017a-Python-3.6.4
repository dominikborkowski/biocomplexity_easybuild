    module load IPython/6.2.1-foss-2017a-Python-3.6.4
}

if { ![ is-loaded configurable-http-proxy/3.1.1-foss-2017a-nodejs-8.9.4 ] } {
    module load configurable-http-proxy/3.1.1-foss-2017a-nodejs-8.9.4
}

prepend-path	LD_LIBRARY_PATH		$root/lib
prepend-path	LIBRARY_PATH		$root/lib
