#%Module
proc ModulesHelp { } {
    puts stderr {

Description
===========
Eigen is a C++ template library for linear algebra:
 matrices, vectors, numerical solvers, and related algorithms.


More information
================
 - Homepage: http://eigen.tuxfamily.org/index.php?title=Main_Page
    }
}

module-whatis {Description: Eigen is a C++ template library for linear algebra:
 matrices, vectors, numerical solvers, and related algorithms.}
module-whatis {Homepage: http://eigen.tuxfamily.org/index.php?title=Main_Page}

set root /apps/easybuild/software/discovery-sandy_bridge/Eigen/3.2.3-foss-2016a

conflict Eigen

if { ![ is-loaded foss/2016a ] } {
    module load foss/2016a
}

prepend-path	CPATH		$root/include
prepend-path	CPLUS_INCLUDE_PATH		$root/include
setenv	EBROOTEIGEN		"$root"
setenv	EBVERSIONEIGEN		"3.2.3"
setenv	EBDEVELEIGEN		"$root/easybuild/Eigen-3.2.3-foss-2016a-easybuild-devel"

# Built with EasyBuild version 3.2.1
