#%Module
proc ModulesHelp { } {
    puts stderr {

Description
===========
OpenBLAS is an optimized BLAS library based on GotoBLAS2 1.13 BSD version.


More information
================
 - Homepage: http://xianyi.github.com/OpenBLAS/
    }
}

module-whatis {Description: OpenBLAS is an optimized BLAS library based on GotoBLAS2 1.13 BSD version.}
module-whatis {Homepage: http://xianyi.github.com/OpenBLAS/}

set root /apps/easybuild/software/discovery-sandy_bridge/OpenBLAS/0.2.20-GCC-6.4.0-2.28

conflict OpenBLAS

if { ![ is-loaded GCC/6.4.0-2.28 ] } {
    module load GCC/6.4.0-2.28
}

prepend-path	CPATH		$root/include
prepend-path	LD_LIBRARY_PATH		$root/lib
prepend-path	LIBRARY_PATH		$root/lib
prepend-path	PATH		$root/bin
prepend-path	PKG_CONFIG_PATH		$root/lib/pkgconfig
setenv	EBROOTOPENBLAS		"$root"
setenv	EBVERSIONOPENBLAS		"0.2.20"
setenv	EBDEVELOPENBLAS		"$root/easybuild/OpenBLAS-0.2.20-GCC-6.4.0-2.28-easybuild-devel"

# Built with EasyBuild version 3.5.1
