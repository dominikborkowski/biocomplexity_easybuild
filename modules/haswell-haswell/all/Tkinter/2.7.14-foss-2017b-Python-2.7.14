#%Module
proc ModulesHelp { } {
    puts stderr {

Description
===========
Tkinter module, built with the Python buildsystem


More information
================
 - Homepage: http://python.org/
    }
}

module-whatis {Description: Tkinter module, built with the Python buildsystem}
module-whatis {Homepage: http://python.org/}

set root /apps/easybuild/software/haswell-haswell/Tkinter/2.7.14-foss-2017b-Python-2.7.14

conflict Tkinter

if { ![ is-loaded foss/2017b ] } {
    module load foss/2017b
}

if { ![ is-loaded Python/2.7.14-foss-2017b ] } {
    module load Python/2.7.14-foss-2017b
}

if { ![ is-loaded Tk/8.6.7-foss-2017b ] } {
    module load Tk/8.6.7-foss-2017b
}

if { ![ is-loaded zlib/1.2.11-GCCcore-6.4.0 ] } {
    module load zlib/1.2.11-GCCcore-6.4.0
}

prepend-path	LD_LIBRARY_PATH		$root/lib
prepend-path	LIBRARY_PATH		$root/lib
setenv	EBROOTTKINTER		"$root"
setenv	EBVERSIONTKINTER		"2.7.14"
setenv	EBDEVELTKINTER		"$root/easybuild/Tkinter-2.7.14-foss-2017b-Python-2.7.14-easybuild-devel"

prepend-path	PYTHONPATH		$root/lib/python2.7
# Built with EasyBuild version 3.5.3
