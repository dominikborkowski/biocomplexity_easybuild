#%Module
proc ModulesHelp { } {
    puts stderr {

Description
===========
HTTP proxy for node.js including a REST API for updating the routing table.
 Developed as a part of the Jupyter Hub multi-user server.


More information
================
 - Homepage: https://github.com/jupyterhub/configurable-http-proxy
    }
}

module-whatis {Description: HTTP proxy for node.js including a REST API for updating the routing table.
 Developed as a part of the Jupyter Hub multi-user server.}
module-whatis {Homepage: https://github.com/jupyterhub/configurable-http-proxy}

set root /apps/easybuild/software/haswell-haswell/configurable-http-proxy/3.1.1-foss-2017a-nodejs-8.9.4

conflict configurable-http-proxy

if { ![ is-loaded foss/2017a ] } {
    module load foss/2017a
}

if { ![ is-loaded nodejs/8.9.4-foss-2017a ] } {
    module load nodejs/8.9.4-foss-2017a
}

prepend-path	LD_LIBRARY_PATH		$root/lib
prepend-path	LIBRARY_PATH		$root/lib
prepend-path	PATH		$root/bin
setenv	EBROOTCONFIGURABLEMINHTTPMINPROXY		"$root"
setenv	EBVERSIONCONFIGURABLEMINHTTPMINPROXY		"3.1.1"
setenv	EBDEVELCONFIGURABLEMINHTTPMINPROXY		"$root/easybuild/configurable-http-proxy-3.1.1-foss-2017a-nodejs-8.9.4-easybuild-devel"

prepend-path	PATH		$root
