#%Module
proc ModulesHelp { } {
    puts stderr {

Description
===========
FreeType 2 is a software font engine that is designed to be small, efficient, highly customizable, and
 portable while capable of producing high-quality output (glyph images). It can be used in graphics libraries, display
 servers, font conversion tools, text image generation tools, and many other products as well.


More information
================
 - Homepage: http://freetype.org
    }
}

module-whatis {Description: FreeType 2 is a software font engine that is designed to be small, efficient, highly customizable, and
 portable while capable of producing high-quality output (glyph images). It can be used in graphics libraries, display
 servers, font conversion tools, text image generation tools, and many other products as well.}
module-whatis {Homepage: http://freetype.org}

set root /apps/easybuild/software/haswell-haswell/freetype/2.7.1-GCCcore-6.3.0-libpng-1.6.29

conflict freetype

if { ![ is-loaded GCCcore/6.3.0 ] } {
    module load GCCcore/6.3.0
}

if { ![ is-loaded bzip2/1.0.6-GCCcore-6.3.0 ] } {
    module load bzip2/1.0.6-GCCcore-6.3.0
}

if { ![ is-loaded libpng/1.6.29-GCCcore-6.3.0 ] } {
    module load libpng/1.6.29-GCCcore-6.3.0
}

if { ![ is-loaded zlib/1.2.11-GCCcore-6.3.0 ] } {
    module load zlib/1.2.11-GCCcore-6.3.0
}

prepend-path	ACLOCAL_PATH		$root/share/aclocal
prepend-path	CPATH		$root/include/freetype2
prepend-path	LD_LIBRARY_PATH		$root/lib
prepend-path	LIBRARY_PATH		$root/lib
prepend-path	MANPATH		$root/share/man
prepend-path	PATH		$root/bin
prepend-path	PKG_CONFIG_PATH		$root/lib/pkgconfig
setenv	EBROOTFREETYPE		"$root"
setenv	EBVERSIONFREETYPE		"2.7.1"
setenv	EBDEVELFREETYPE		"$root/easybuild/freetype-2.7.1-GCCcore-6.3.0-libpng-1.6.29-easybuild-devel"

# Built with EasyBuild version 3.6.0
