#%Module
proc ModulesHelp { } {
    puts stderr {

Description
===========
Python Protocol Buffers runtime library.


More information
================
 - Homepage: https://github.com/google/protobuf/
    }
}

module-whatis {Description: Python Protocol Buffers runtime library.}
module-whatis {Homepage: https://github.com/google/protobuf/}

set root /apps/easybuild/software/haswell-haswell/protobuf-python/3.4.0-foss-2017b-Python-3.6.3

conflict protobuf-python

if { ![ is-loaded foss/2017b ] } {
    module load foss/2017b
}

if { ![ is-loaded Python/3.6.3-foss-2017b ] } {
    module load Python/3.6.3-foss-2017b
}

if { ![ is-loaded protobuf/3.4.0-foss-2017b ] } {
    module load protobuf/3.4.0-foss-2017b
}

prepend-path	PATH		$root/bin
setenv	EBROOTPROTOBUFMINPYTHON		"$root"
setenv	EBVERSIONPROTOBUFMINPYTHON		"3.4.0"
setenv	EBDEVELPROTOBUFMINPYTHON		"$root/easybuild/protobuf-python-3.4.0-foss-2017b-Python-3.6.3-easybuild-devel"

prepend-path	PYTHONPATH		$root/lib/python3.6/site-packages
