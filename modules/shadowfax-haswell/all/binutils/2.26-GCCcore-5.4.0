#%Module
proc ModulesHelp { } {
    puts stderr { binutils: GNU binary utilities - Homepage: http://directory.fsf.org/project/binutils/
    }
}

module-whatis {Description: binutils: GNU binary utilities - Homepage: http://directory.fsf.org/project/binutils/}

set root /apps/easybuild/software/shadowfax-haswell/binutils/2.26-GCCcore-5.4.0

conflict binutils

if { ![ is-loaded GCCcore/5.4.0 ] } {
    module load GCCcore/5.4.0
}

prepend-path	CPATH		$root/include
prepend-path	LD_LIBRARY_PATH		$root/lib
prepend-path	LIBRARY_PATH		$root/lib
prepend-path	MANPATH		$root/share/man
prepend-path	PATH		$root/bin
setenv	EBROOTBINUTILS		"$root"
setenv	EBVERSIONBINUTILS		"2.26"
setenv	EBDEVELBINUTILS		"$root/easybuild/binutils-2.26-GCCcore-5.4.0-easybuild-devel"

# Built with EasyBuild version 3.1.2
