    }
}

module-whatis {Description: GD.pm - Interface to Gd Graphics Library - Homepage: http://search.cpan.org/~lds/GD/}

set root /apps/easybuild/software/shadowfax-haswell/GD/2.52-foss-2016a-Perl-5.22.1

conflict GD

if { ![ is-loaded foss/2016a ] } {
    module load foss/2016a
}

if { ![ is-loaded Perl/5.22.1-foss-2016a ] } {
    module load Perl/5.22.1-foss-2016a
}

