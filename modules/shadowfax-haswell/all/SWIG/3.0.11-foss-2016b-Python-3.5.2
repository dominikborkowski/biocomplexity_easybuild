#%Module
proc ModulesHelp { } {
    puts stderr {

Description
===========
SWIG is a software development tool that connects programs written in C and C++ with
 a variety of high-level programming languages.


More information
================
 - Homepage: http://www.swig.org/
    }
}

module-whatis {Description: SWIG is a software development tool that connects programs written in C and C++ with
 a variety of high-level programming languages.}
module-whatis {Homepage: http://www.swig.org/}

set root /apps/easybuild/software/shadowfax-haswell/SWIG/3.0.11-foss-2016b-Python-3.5.2

conflict SWIG

if { ![ is-loaded foss/2016b ] } {
    module load foss/2016b
}

if { ![ is-loaded Python/3.5.2-foss-2016b ] } {
    module load Python/3.5.2-foss-2016b
}

if { ![ is-loaded PCRE/8.38-foss-2016b ] } {
    module load PCRE/8.38-foss-2016b
}

prepend-path	PATH		$root/bin
setenv	EBROOTSWIG		"$root"
setenv	EBVERSIONSWIG		"3.0.11"
setenv	EBDEVELSWIG		"$root/easybuild/SWIG-3.0.11-foss-2016b-Python-3.5.2-easybuild-devel"

