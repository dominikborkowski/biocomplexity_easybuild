set root /apps/easybuild/software/shadowfax-haswell/entrypoints/0.2.2-foss-2016b-Python-3.5.2

conflict entrypoints

if { ![ is-loaded foss/2016b ] } {
    module load foss/2016b
}

if { ![ is-loaded Python/3.5.2-foss-2016b ] } {
    module load Python/3.5.2-foss-2016b
}

if { ![ is-loaded configparser/3.5.0-foss-2016b-Python-3.5.2 ] } {
    module load configparser/3.5.0-foss-2016b-Python-3.5.2
}

setenv	EBROOTENTRYPOINTS		"$root"
setenv	EBVERSIONENTRYPOINTS		"0.2.2"
setenv	EBDEVELENTRYPOINTS		"$root/easybuild/entrypoints-0.2.2-foss-2016b-Python-3.5.2-easybuild-devel"

prepend-path	PYTHONPATH		$root/lib/python3.5/site-packages
