sanity_check_paths = {
    'files': [],
    'dirs': [("%s-%%(version)s-py%s.egg" % (pylibdir, pyshortver),
               "%s-%%(version)s-py%s-linux-x86_64.egg" % (pylibdir, pyshortver))],
}

# module name happens to be case sensitive
options = {'modulename': '%(name)s' }

moduleclass = 'devel'


